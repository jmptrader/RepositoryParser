<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:RepositoryParser.Resources.Themes">



    <LinearGradientBrush x:Key="GradientMainColor" StartPoint="0,0" EndPoint="1,1" Opacity="1">
        <LinearGradientBrush.GradientStops>
            <GradientStopCollection>
                <GradientStop Color="#143821" Offset="0" />
                <GradientStop Color="#1a472a" Offset="1" />
            </GradientStopCollection>
        </LinearGradientBrush.GradientStops>
    </LinearGradientBrush>
    
    <SolidColorBrush x:Key="AppSecondMainColor" Color="#aaaaaa" />
    <SolidColorBrush x:Key="AppMainColor" Color="#1a472a" />
    <SolidColorBrush x:Key="DarkGray" Color="#5d5d5d"/>
    <SolidColorBrush x:Key="LightGray" Color="#666666"/>
   <!-- <SolidColorBrush x:Key="AppMainColor" Color="#336699" />-->
    <SolidColorBrush x:Key="Grey" Color="#FAFAFA" />
    <SolidColorBrush x:Key="GitOrange" Color="#F34F29"/>
    <SolidColorBrush x:Key="SvnBlue" Color="#809CC8"/>
    <SolidColorBrush x:Key="ChangesGreen" Color="#6BBC7D"/>
    <SolidColorBrush x:Key="ChangesRed" Color="#FC7979"/>
    <SolidColorBrush x:Key="ChangesBlue" Color="#74BBC9"/>
    <SolidColorBrush x:Key="MyRowColor" Color="#C0C0C0"/>

</ResourceDictionary>